rable\r\n                  :items=\"items\"\r\n                  label=\"Caracteristique\"\r\n                  v-model=\"carac.caracName\"\r\n                ></v-autocomplete>\r\n              </v-col>\r\n              <v-col cols=\"18\" md=\"5\">\r\n                <v-text-field\r\n                  label=\"Valeur\"\r\n                  required\r\n                  v-model=\"carac.caracValue\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"18\" md=\"1\" style=\"padding-top: 30px\">\r\n                <v-btn depressed small dark @click=\"removeCarac(counter)\">\r\n                  <v-icon dark left> mdi-minus-circle </v-icon>\r\n                  Retirer\r\n                </v-btn>\r\n              </v-col>\r\n            </v-row>\r\n          </div>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-card-actions>\r\n        <v-btn color=\"success\" @click=\"addItem\" v-if=\"listCaracs.length > 0\"\r\n          >Ajouter l'item</v-btn\r\n        >\r\n        <v-spacer />\r\n        <v-btn @click=\"addCarac\">Ajouter une caracteristique</v-btn>\r\n        <v-spacer />\r\n        <v-btn elevation=\"3\" color=\"error\" @click=\"$emit('input', false)\"\r\n          >Fermer</v-btn\r\n        >\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n<script>\r\n//import PostService from \"../PostService\";\r\n\r\nexport default {\r\n  name: \"add-item\",\r\n  props: [\"value\"],\r\n  methods: {\r\n    async addItem() {\r\n      let res = {};\r\n      res[\"name\"] = this.name;\r\n      for (const carac of this.listCaracs) {\r\n        let nom = carac.caracName;\r\n        let value = carac.caracValue;\r\n        res[nom] = value;\r\n      }\r\n      await PostService.insertPost(res);\r\n    },\r\n    addCarac() {\r\n      this.listCaracs.push({ caracName: \"\", caracValue: \"\" });\r\n    },\r\n    removeCarac(counter) {\r\n      this.listCaracs.splice(counter, 1);\r\n    },\r\n  },\r\n  data: () => ({\r\n    name: \"\",\r\n    listCaracs: [],\r\n    items: [\r\n      \"PA\",\r\n      \"PM\",\r\n      \"Initiative\",\r\n      \"Vitalité\",\r\n      \"PO\",\r\n      \"Invocation\",\r\n      \"Prospection\",\r\n      \"Sagesse\",\r\n      \"Force\",\r\n      \"Intelligence\",\r\n      \"Chance\",\r\n      \"Agilité\",\r\n      \"Esquive PA\",\r\n      \"Esquive PM\",\r\n      \"Tacle\",\r\n      \"Fuite\",\r\n      \"Puissance\",\r\n      \"Dommages\",\r\n      \"Dommages Force\",\r\n      \"Dommages Intelligence\",\r\n      \"Dommages Chance\",\r\n      \"Dommages Agilité\",\r\n      \"Dommages Neutre\",\r\n      \"Dommages distance\",\r\n      \"Dommages poussée\",\r\n      \"Dommages pièges\",\r\n      \"Dommages mêlée\",\r\n      \"Dommages sort\",\r\n      \"Pods\",\r\n      \"Critiques\",\r\n      \"Soins\",\r\n      \"Renvoi\",\r\n      \"Poussée\",\r\n      \"Resistance % Force\",\r\n      \"Resistance % Intelligence\",\r\n      \"Resistance % Chance\",\r\n      \"Resistance % Agilité\",\r\n      \"Resistance % Neutre\",\r\n      \"Resistance fixe Force\",\r\n      \"Resistance fixe Intelligence\",\r\n      \"Resistance fixe Chance\",\r\n      \"Resistance fixe Agilité\",\r\n      \"Resistance fixe Neutre\",\r\n      \"Esquive PA\",\r\n      \"Esquive PM\",\r\n      \"Retrait PA\",\r\n    ],\r\n  }),\r\n};\r\n</script>"]}]}